<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/style.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@600&display=swap" rel="stylesheet">
    <title>Document</title>
</head>
<body>
    <!-- header -->
    <nav>
        <div class="logo-nav">
            <a href="/">
                <img src="img/logo.png" alt="logo-tupai">
                <p>Shape</p>
            </a>
        </div>
        <div class="user">
            <a href="/login">
                <p>Sign in</p>
                <img src="img/user.png"/>
            </a>
        </div>
    </nav>


    <!-- main -->
    <main class="catalog">
        <div class="filter-nav anim">
            <div class="filter-sort anim">
                <a href="#">Most Popular</a>
                <a href="#">Latest</a>
            </div>
            <div class="search anim">
                <form action="#">
                    <input type="text" placeholder=" Search "name="search">
                        <img class="fa"src="img/Search.png">
                </form>
            </div>
            <div class="filter-categories anim">
                <a href="#">3D</a>
                <a href="#">Vector</a>
                <a href="#">Raster</a>
            </div>
        </div>
        <div class="content">
        <% for( let index = 0; index < data.data.length; index++ ) { %>
                <div class="item-cart">
                    <img src="<%= data.data[index].preview[0].url%>" alt="">
                    <div class="download-button">
                        <a href="/detail">Download
                            <img src="img/download.png" alt="">
                        </a>
                    </div>
                </div>
        <% } %>
    </div>
    </main>


    <!-- footer -->
    <footer>
        <div class="nav-footer">
            <div>
                <p>@Tupailabs.com</p>
            </div>
            <div class="social">
                <a href="#">
                    <img src="img/facebook.png" alt="logo-tupai">
                </a>
                <a href="#">
                    <img src="img/instagram.png" alt="logo-tupai">
                </a>
            </div>
        </div>
    </footer>
</body>
</html>